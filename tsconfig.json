{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "es2018",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true
  }
}
